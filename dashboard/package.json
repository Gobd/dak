{
  "name": "dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite build --mode development && vite preview --port 8080",
    "lint": "eslint .",
    "lint:sh": "shellcheck scripts/*.sh",
    "typecheck": "echo 'No typecheck configured'",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "check": "pnpm lint && pnpm lint:sh && pnpm format:check && pnpm typecheck && pnpm build"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "eslint": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "shellcheck": "^4.1.0",
    "vite": "catalog:vite"
  }
}
